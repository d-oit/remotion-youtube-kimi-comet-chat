{
  "name": "kimi-comet-chat-video",
  "version": "1.0.0",
  "description": "Stunning Remotion YouTube video for Kimi Comet Chat $0.99 Series",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "remotion vite",
    "build": "remotion render src/Root.tsx MainVideo out/video.mp4",
    "build-intro": "remotion render src/Root.tsx Intro out/intro.mp4",
    "build-segment1": "remotion render src/Root.tsx Segment1 out/segment1.mp4",
    "build-segment2": "remotion render src/Root.tsx Segment2 out/segment2.mp4",
    "build-segment3": "remotion render src/Root.tsx Segment3 out/segment3.mp4",
    "build-outro": "remotion render src/Root.tsx Outro out/outro.mp4",
    "build-thumbnail": "remotion still src/Root.tsx MainThumbnail out/thumbnail-main.png --overwrite",
    "build-thumbnail-short": "remotion still src/Root.tsx ShortThumbnail out/thumbnail-short.png --overwrite",
    "build-thumbnail-v3": "remotion still src/Root.tsx MainThumbnailV3 out/thumbnail-main.png --overwrite",
    "build-thumbnail-short-v3": "remotion still src/Root.tsx ShortThumbnailV3 out/thumbnail-short.png --overwrite",
    "build-thumbnails-v3": "npm run build-thumbnail-v3 && npm run build-thumbnail-short-v3",
    "build-thumbnail-v4": "remotion still src/Root.tsx MainThumbnailV4 out/thumbnail-main-v4.png --overwrite",
    "build-thumbnail-short-v4": "remotion still src/Root.tsx ShortThumbnailV4 out/thumbnail-short-v4.png --overwrite",
    "build-thumbnails-v4": "npm run build-thumbnail-v4 && npm run build-thumbnail-short-v4",
    "build-all-thumbnails": "npm run build-thumbnails-v3 && npm run build-thumbnails-v4",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@remotion/bundler": "^4.0.410",
    "@remotion/cli": "^4.0.410",
    "@remotion/player": "^4.0.410",
    "@remotion/renderer": "^4.0.410",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remotion": "^4.0.410"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0"
  }
}
